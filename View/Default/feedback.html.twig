{% extends 'layout.html.twig' %}
{% block content %}
<h1>Welcome</h1>

<form method="post">
    <input name="email" type="email" placeholder="Email" class="form-control" value="{{ form.email }}"><br>
    <textarea name="message" class="form-control" placeholder="Your message"></textarea><br>
    <button class="btn btn-primary">Go</button><br>
</form>
{% endblock %}